.container {
  padding: 20rpx;
}

.header {
  margin-bottom: 20rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
}

.parking-list {
  display: flex;
  flex-direction: column;
}

.parking-item {
  background-color: #f8f8f8;
  border-radius: 10rpx;
  padding: 20rpx;
  margin-bottom: 20rpx;
}

.parking-name {
  font-size: 32rpx;
  font-weight: bold;
}

.parking-available, .parking-price {
  font-size: 28rpx;
  color: #666;
  margin-top: 10rpx;
}